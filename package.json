{
  "name": "next-starter-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "preview": "serve dist",
    "lint": "next lint",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest coverage",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "tool:auto-translate-data": "node ./tools/auto-translate-data.mjs",
    "predeploy": "npm run build && npm run tool:create-cv",
    "deploy": "gh-pages -d dist",
    "only-deploy": "gh-pages -d dist",
    "tool:create-cv": "node ./tools/create-cv/index.mjs",
    "tool:generate-svg": "node ./tools/generate-svg.mjs",
    "tool:optimize-images": "node ./tools/optimize-images.mjs",
    "prepare": "husky install",
    "analyze": "set ANALYZE=1 && next build"
  },
  "dependencies": {
    "@hookform/resolvers": "^4.1.3",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.1.5",
